{
    "manifest_version": 2,
    "name":"Budget Manager",
    "version":"1.1",
    "description":"Manages your Budget",
    "icons":{
        "128":"./icon128.png",
        "48":"./icon48.png",
        "16":"./icon16.png"
    },
    "browser_action":{
        "default_icon":"./icon16.png",
        "default_popup":"./popBudget.html"
    }, 
    "options_page":"options.html",
    "permissions":[
        "storage",
        "notifications"
    ]
}